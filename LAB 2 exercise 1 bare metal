#include <avr/io.h>
#include <avr/interrupt.h>


ISR(INT0_vect) {
    PORTB ^= (1 << PB5); // Toggle LED on PB5
}

int main(void) {

    DDRB |= (1 << PB5);

    DDRD &= ~(1 << PD2);

    PORTD |= (1 << PD2);

    EICRA |= (1 << ISC01);
    EICRA &= ~(1 << ISC00);

    EIMSK |= (1 << INT0);

    sei();

    while (1) {
    
    }
}
