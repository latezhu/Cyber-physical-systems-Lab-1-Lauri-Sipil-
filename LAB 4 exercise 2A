#define F_CPU 16000000UL
#include <avr/io.h>
#include <util/delay.h>

int main(void)
{
	DDRB |= (1 << PB5);

	while (1)
	{
		PORTB &= ~(1 << PB5);
		_delay_ms(15000);

		PORTB |= (1 << PB5);
		_delay_ms(1000);

		PORTB &= ~(1 << PB5);
		_delay_ms(50);
		PORTB |= (1 << PB5);
		_delay_ms(950);

		PORTB &= ~(1 << PB5);
		_delay_ms(100);
		PORTB |= (1 << PB5);
		_delay_ms(900);

		PORTB &= ~(1 << PB5);
		_delay_ms(150);
		PORTB |= (1 << PB5);
		_delay_ms(850);

		PORTB &= ~(1 << PB5);
		_delay_ms(200);
		PORTB |= (1 << PB5);
		_delay_ms(800);

		PORTB &= ~(1 << PB5);
	}
}
